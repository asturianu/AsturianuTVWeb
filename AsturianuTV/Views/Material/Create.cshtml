@model AsturianuTV.Infrastructure.Data.Models.Material
@{
    Layout = "_AdminLayout";
}

<form class="admin-form" asp-action="Create" asp-controller="Material" enctype="multipart/form-data">
    <h2 class="form-title">Create material</h2>
    <div class="form-group">
        <label asp-for="FilePath">Path</label>
        <input type="text" asp-for="FilePath">
    </div>
    <div class="form-group">
        <button class="save-button" type="submit">
            Save
        </button>
    </div>
</form>